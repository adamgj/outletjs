{
  "name": "device-test",
  "version": "0.3.0",
  "description": "test device",
  "device": {
    "conf": {
      "test": "value",
      "increment": {
        "step": 1
      }
    },
    "workflows": [
      {
        "trigger": "math/count/inc",
        "action": "increment"
      },
      {
        "trigger": "dontRun",
        "action": "dontRun"
      }
    ],
    "listeners": [
      {
        "emit": "on",
        "trigger": "connect",
        "action": "deviceConnect"
      },
      {
        "trigger": "test",
        "action": "test"
      }
    ]
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "outlet",
    "test"
  ],
